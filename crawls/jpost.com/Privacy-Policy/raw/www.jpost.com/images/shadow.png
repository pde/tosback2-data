         var SymbolOne = '_';
         var SymbolTwo = '-';
         var SymbolNone = '';
         var LeadString = '&nbsp;&nbsp;&nbsp;';
         var Headlines = new Array();
         var Links = new Array();
         var Time = new Array();
         var ItemCount = 8;
 

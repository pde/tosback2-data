//--------------------------------------
//--------------------------------------
//--------- User Registration  ---------
//--------------------------------------
//--------------------------------------

function validateEmail(email, template) {
    var valid = checkMail(email);
    var errorDivName = template+'ErrorDiv';
    updateErrorDiv(errorDivName, 'Please enter a valid email address.', valid);
    return valid;
}

function submitRegisterUser(template, source) {
    var emailValid = true;
    var inputName = "inputEmailSignup"+template;
    var emailInput = $('#'+inputName);

    if (emailInput) {
        registrationForm.email = $(emailInput).val();
        registrationForm.template = template;
        registrationForm.registrationSource = source;
        emailValid = validateEmail(registrationForm.email, template);
    }
    
    if (emailValid) {
    	registrationForm.optIn = true;
        var divName = "emailSignup" + template;
        $.ajax({
            url: '/reg/public/register',
            dataType: "html",
            data: registrationForm,
            context:this,
            success: function (data) {$('#'+divName).html(data);}
        });
        
        $('#'+divName).text('Processing registration...');
    }
};

function updateErrorDiv(divName, msg, valid) {
    if (!valid) {
        $('#' + divName).show();
        $('#' + divName).text(msg);
    }
    else if ($('#' + divName)) {
        $('#' + divName).hide();
        $('#' + divName).text('');
    }
};

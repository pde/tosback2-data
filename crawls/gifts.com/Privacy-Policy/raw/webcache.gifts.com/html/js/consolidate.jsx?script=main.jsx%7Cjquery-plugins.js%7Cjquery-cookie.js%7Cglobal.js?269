	function linkCodeFinder(obj,linkName) {
        var s=s_gi('gc-giftscom');
        s.linkTrackVars='prop22';
        s.linkTrackEvents='None';
        s.prop22=linkName;
        var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
    }
	function linkCodeChannelFinder(obj, linkPrefix) {
        var s=s_gi('gc-giftscom');
        channel = (s != null && s.channel != "") ? s.channel : '';
        linkCodeFinder(obj, linkPrefix+': '+channel);
    }
    function linkCodeSLTracker(obj, linkPrefix) {
    	var s=s_gi('gc-giftscom');
        channel = (s != null && s.channel != "") ? s.channel : '';
        s.linkTrackVars='events';
        s.linkTrackEvents='event20';
        s.events='event20';
        var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',channel); }
    }
	function linkCodeFBConnect(obj,linkName) {
        var s=s_gi('gc-giftscom');
        s.linkTrackVars='eVar11';
        s.linkTrackEvents='event14';
        s.eVar11=linkName;
        var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
    }

    function linkCodeQuizAnswers(obj,linkName) {
        var s=s_gi('gc-giftscom');
        s.linkTrackVars ='eVar14';
        s.linkTrackEvents ='None';
        s.prop17 = ' | ';
        s.eVar12 = ' |  | Q: A:linkName}'
        s.eVar14 = '';
        var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
    }


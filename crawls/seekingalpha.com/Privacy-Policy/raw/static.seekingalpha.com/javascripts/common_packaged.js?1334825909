if(aSymbolsWL.length==3)return aSymbolsWL;}
if(aSymbolsWL.length)aSymbolsWL=aSymbolsWL.concat(aSymbols.slice(0,3-aSymbolsWL.length));return aSymbolsWL.sort();}
return false;}

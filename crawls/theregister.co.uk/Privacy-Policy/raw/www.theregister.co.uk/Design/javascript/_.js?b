.before('<div id=google-reg-map></div>');ancestors.filter(':empty').each(function(){$(this).remove()});var map=new google.maps.Map(document.getElementById("google-reg-map"),{zoom:8,center:new google.maps.LatLng(57.596667,-13.688611),mapTypeId:google.maps.MapTypeId.ROADMAP,mapTypeControl:true,navigationControl:true,scaleControl:true});var bound=new google.maps.LatLngBounds();set_gicon(type);function plot_map_marks(Points,line_colour){if(typeof Points=='undefined')return;if(typeof line_colour=='undefined')line_colour='#ff0000';var a=[];for(var i in Points){var coords=Points[i].coords.split(',',3);Points[i].latlng=new google.maps.LatLng(coords[1],coords[0]);bound.extend(Points[i].latlng);make_mark(map,Points[i],type);a.push(Points[i].latlng);}

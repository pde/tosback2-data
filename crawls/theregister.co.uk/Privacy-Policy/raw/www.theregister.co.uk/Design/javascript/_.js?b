function polling(id){document.write('<div id=poll></div>');polling_setup(id);}
function polling_setup(id){var already_voted=0;if(typeof(localStorage)=='undefined'){var voted_polls_list=[];if(($.cookie('voted_polls')!='')&&($.cookie('voted_polls')!='undefined')){voted_polls_list=$.cookie('voted_polls');var cookie_polls=voted_polls_list.split(",");if(jQuery.inArray(id.toString(),cookie_polls)>-1)already_voted++;}}
else{if(localStorage.getItem('voted_polls')===null){var voted_polls_list=[];}
else{var voted_polls_list=JSON.parse(localStorage['voted_polls']);}
if(jQuery.inArray(id,voted_polls_list)>-1)already_voted++;}
$.ajax({url:'/Polls/poll/'+id,dataType:'json',success:function(data){$('#poll').data({'data':data,'poll_id':id,'prev_polls':voted_polls_list,'mode':'questions'});if(already_voted)$('#poll').data('mode','results');render_poll(0);}});$(document).on('click','#poll_submit_btn',function(e){if($(this).hasClass('poll_submit')){submit_poll_votes(get_poll_votes())}
else{if(!$('#poll .responses:visible input:checked').length){$('#poll ul .current').addClass('q_not_answered');}
move_poll_question(1);}
return false;});$(document).on('click','#poll input[type="radio"]',function(e){$('#poll ul .current').addClass('q_answered').removeClass('q_not_answered');var question_id=$(this).parents('.responses').data('question_id');var votes=get_poll_votes();votes[question_id]=$(this).val();$('#poll').data('votes',votes);if(!$('#poll_submit_btn').hasClass('poll_submit'))move_poll_question(1);});$(document).on('click','#poll .questions li',function(e){if($('#poll .responses').length){if(!$(this).is('.q_answered, .q_not_answered'))return;if(!$('#poll .responses:visible input:checked').length){$('#poll ul .current').addClass('q_not_answered');}}
var new_position=$(this).text()-1;$('#poll').data('position',new_position);render_poll(new_position);});$(document).on('click','#poll .view_results',function(){render_poll(0)});}
function render_thanks(){var poll=$('#poll');var domain=location.hostname.split('www.')[1];poll.data('mode','results');poll.data('position',1);poll.html('');poll.append($('<h2>Thanks, the results will be updated shortly!</h2>'),$('<p>Want to sign up for <a href="http://account.'+domain+'/?'
+'product=tech_panel" target=_blank>Reader Surveys?</a></p>'),$('<p class=view_results>View the results for this poll</p>'));poll.css('min-height',poll.data('full_height')+'px');}
function render_poll(position){var poll=$('#poll');var data=poll.data('data');var mode=poll.data('mode');var question=data.questions[position];var question_count=poll.data('data').questions.length;if(!$('#poll .questions').length){poll.data('position',position).html('');var questions=$('<ul class=questions></ul>');$.each(data.questions,function(idx,val){var tab=$('<li></li>').text(idx+1);if(idx==position)tab.addClass('current');questions.append(tab);});var poll_title=$('<h2></h2>').text(data.title);poll.append(poll_title,questions);poll.data('height',0);$.each(data.questions,function(id,val){var container=$('<div></div>').addClass('poll_questions').append($('<h3></h3>').text(val.title));poll.append(container);if(mode=='results')render_poll_result(container,data.questions[id]);else render_poll_question(container,data.questions[id]);});$('.poll_questions').css('min-height',poll.data('height')+'px');poll.append($('<input type="submit" class="style_btn" id="poll_submit_btn" name="vote" value="Skip"></input>'));}
else{$('#poll .questions li').removeClass('current');$('#poll .questions li:eq('+position+')').addClass('current');}
if($.browser.msie&&$.browser.version<9){$('#poll h3').text(question.title);$('.show_poll_q').hide();$('.poll_questions').removeClass('show_poll_q');$('.poll_questions:eq('+position+')').addClass('show_poll_q');$('.show_poll_q').show();}
else{$('.show_poll_q').stop().fadeOut(150,function(){$('#poll h3').text(question.title);$('.poll_questions').removeClass('show_poll_q');$('.poll_questions:eq('+position+')').addClass('show_poll_q');$('.show_poll_q').stop().fadeIn(350);$('.show_poll_q').css('opacity',1);});if(!$('.show_poll_q').length){$('.poll_questions:eq('+position+')').addClass('show_poll_q');$('.show_poll_q').stop().fadeIn(350);}}
if($('#poll .responses').length){var quesion_number=position+1;if(quesion_number==question_count){$('#poll_submit_btn').attr('value','Submit').addClass('poll_submit');}
else{$('#poll_submit_btn').attr('value','Skip').removeClass('poll_submit');}}
else{$('#poll_submit_btn').addClass('poll_hide_btn');$('#poll .questions li').addClass('results_nav');}
poll.data('full_height',poll.height());}
function render_poll_question(container,question){var responses=$('<ul class=responses></ul>').data('question_id',question.id);$.each(question.responses,function(idx,val){var input=$('<input type=radio name=poll'+question.id+'></input>').attr('value',val.id);responses.append($('<li></li>').append($('<label></label>').text(val.title).prepend(input)));});container.append(responses);var height=container.height();if(height>$('#poll').data('height'))$('#poll').data('height',height);}
function render_poll_result(container,question){var responses=$('<ul class=results></ul>');var votes_count=0;$.each(question.responses,function(idx,val){votes_count=votes_count+parseFloat(val.votes);});var votes_percent=100/votes_count;$.each(question.responses,function(idx,val){var calc_vote=votes_percent*parseFloat(val.votes);var poll_text=$('<div></div>').addClass('poll_text').text(val.votes);var poll_bar=$('<div></div>').addClass('resultBarFill');if(calc_vote<50){if($.browser.msie&&$.browser.version==7.0){poll_text.css({float:'left','padding-left':(calc_vote*2)+'px'});}
else{poll_text.css({float:'left','margin-left':calc_vote+'%','padding-left':'2px'});}
if(calc_vote<1){if(calc_vote==0)poll_bar.css('border','0px');calc_vote=1;}
poll_bar.css('width',(calc_vote-1)+'%');}
else{poll_text.css({float:'right',color:'#fff','padding-right':'4px'});if(location.hostname.indexOf("reghardware")!=-1)poll_text.css('color','#555');if($.browser.msie&&$.browser.version==7.0){poll_text.css('padding-left',(calc_vote*2-45)+'px');}
else{poll_text.css('margin-right',100-calc_vote+'%');}
poll_bar.css('width',(calc_vote-1)+'%');}
var response=$('<div></div>').addClass('formRow').append($('<div></div>').addClass('result_ques').text(val.title),$('<div></div>').addClass('resultBar').append(poll_text,poll_bar));responses.append(response);});container.append(responses);var height=container.height()-30;if(height>$('#poll').data('height'))$('#poll').data('height',height);}
function get_poll_votes(){var votes=$('#poll').data('votes');return typeof(votes)!='undefined'?votes:{};}
function update_poll_votes(){var votes=$('#poll').data('votes');var json_data=$('#poll').data('data');$.each(votes,function(ques,voted_id){$.each(json_data.questions,function(ques_id,val){if(val.id.indexOf(ques)==-1)return;$.each(val.responses,function(resp_id,valx){if(valx.id.indexOf(voted_id)==-1)return;var vote_num=parseInt(json_data.questions[ques_id].responses[resp_id].votes)+1;json_data.questions[ques_id].responses[resp_id].votes=vote_num;});});});$('#poll').data('data',json_data);}
function move_poll_question(delta){var new_position=$('#poll').data('position')+delta;$('#poll').data('position',new_position);render_poll(new_position);}
function submit_poll_votes(votes){var poll=$('#poll').data('mode','results');var poll_id=$('#poll').data('poll_id');var prev_polls=$('#poll').data('prev_polls');if(typeof(localStorage)=='undefined'){prev_polls=prev_polls+','+poll_id;$.setcookie('voted_polls',prev_polls,{expires:365});}
else{prev_polls.push(poll_id);localStorage['voted_polls']=JSON.stringify(prev_polls);}
$.ajax({url:'/Polls/poll/'+poll_id,dataType:'json',success:function(data){poll.data('data',data);update_poll_votes();$.ajax({'type':'POST','url':'/Polls/vote/'+poll_id,'dataType':'json','data':votes,'success':function(data){render_thanks();}});}});}
function slideshow(id){document.write('<div id="slideshow'+id+'" class="slideshow"></div>');slideshow_setup(id);}
function slideshow_setup(id){var parent='#slideshow'+id;$(parent).css('opacity',0);var rhs=slideshow_pos(parent);$.getJSON('/Slideshow/show/'+id,function(json){$(parent).data("slide",{slide_pos:0,json:json});var left_arrow='<div class="slide_left"><img src="/Design/graphics/icons/slideshow_left_bg.png" alt="slide left"></div>';var right_arrow='<div class="slide_right"><img src="/Design/graphics/icons/slideshow_right_bg.png" alt="slide right"></div>';if(rhs){$(parent).append($('<h3></h3>').text(json.slides[0].title),$('<div class="slide_img"></div>').html(left_arrow+right_arrow),$('<div class="slide_desc"></div>').text(json.slides[0].description));}
else{$(parent).append($('<h3></h3>').text(json.slides[0].title),$('<div class="slide_img"></div>').html(left_arrow+right_arrow),$('<div class="slide_rail"></div>').html(left_arrow+right_arrow).append($('<div class="slide_rail_images"></div>')),$('<div class="slide_desc"></div>').text(json.slides[0].description));}
load_slide_img(1,parent);load_slide_img(2,parent);$(parent+' .slide_pic').eq(0).addClass('current_slide_img');$('.slide_left img').hide();if(json.slides.length==1)$('.slide_right img').hide();if(!rhs)load_slide_rail(json,parent);$(parent).css('opacity',1);});$(parent).mouseover(function(){$(document).keydown(function(e){var json=$(parent).data('slide').json;var new_slide=$(parent).data('slide').slide_pos;switch(e.keyCode){case 37:if(new_slide-1<0)break;change_slide(new_slide-1,parent);break;case 39:if(new_slide+1==json.slides.length)break;change_slide(new_slide+1,parent);break;}});});$(parent).mouseout(function(){$(document).off('keydown')});}
function load_slide_rail(json,parent){var slide_display='';var max_rail_size=rail_size(parent);$.each(json.slides,function(n){if(n>max_rail_size)slide_display='none';var rail_img=json.slides[n].media.url;$(parent+' .slide_rail_images').append($('<img></img>').attr({'src':'http://www.regmedia.co.uk/'+rail_img+'?x=50&y=50','alt':'slide rail image'})
.css('display',slide_display));});$(parent+' .slide_rail_images img').eq(0).addClass('current_rail_slide');}
function load_slide_img(slide_num,parent){var json=($(parent).data('slide').json);slide_num--;if(slide_num>=json.slides.length)return;var slide_img=json.slides[slide_num].media.url;$(parent+' .slide_img').append($('<img></img>').attr({'src':'http://www.regmedia.co.uk/'+slide_img,'alt':'slide image'})
.addClass('slide_pic slide'+slide_num));}
$(function(){$('body').on('click','.slide_rail_images img',function(){var parent='#'+$(this).parents('.slideshow').attr('id');var new_slide=$(this).index();change_slide(new_slide,parent);});$('body').on('click','.slide_left img',function(){var parent='#'+$(this).parents('.slideshow').attr('id');var new_slide=$(parent).data('slide').slide_pos-1;change_slide(new_slide,parent);});$('body').on('click','.slide_right img',function(){var parent='#'+$(this).parents('.slideshow').attr('id');var new_slide=$(parent).data('slide').slide_pos+1;change_slide(new_slide,parent);});});function change_slide(slide_num,parent){var prev_rail_slide=$(parent+' .current_rail_slide').index();var json=($(parent).data('slide').json);var slide_total=json.slides.length;var img_num=slide_num+1;var prev_slide=slide_num-1;var rhs=slideshow_pos(parent);$(parent+' .current_slide_img').css('display','none');$(parent+' img').removeClass('current_slide_img current_rail_slide');if($(parent+' .slide'+prev_slide).size()<1&&prev_slide>0)load_slide_img(img_num-1,parent);if($(parent+' .slide'+slide_num).size()<1)load_slide_img(img_num,parent);if($(parent+' .slide'+img_num).size()<1&&img_num<slide_total)load_slide_img(img_num+1,parent);if(!rhs){var max_rail_size=rail_size(parent);var first_disp_slide=$(parent+' .slide_rail_images img:visible').eq(0).index();if(slide_num>=max_rail_size&&slide_num>prev_rail_slide){var slide_total=json.slides.length;if(slide_num+1<slide_total){$(parent+' .slide_rail_images img').eq(slide_num-max_rail_size).hide();$(parent+' .slide_rail_images img').eq(slide_num+1).show();}}
else if(slide_num>0&&slide_num==first_disp_slide){$(parent+' .slide_rail_images img').eq(slide_num-1).show();$(parent+' .slide_rail_images img').eq(slide_num+max_rail_size).hide();}
$(parent+' .slide_rail_images img').eq(slide_num).addClass('current_rail_slide');}
$(parent+' h3').text(json.slides[slide_num].title);$(parent+' .slide_desc').text(json.slides[slide_num].description);$(parent+' .slide'+slide_num).addClass('current_slide_img');$(parent).data("slide",{slide_pos:slide_num,json:json});if($(parent+' .slide_right:hidden'))$(parent+' .slide_right img').show();if($(parent+' .slide_left:hidden'))$(parent+' .slide_left img').show();if(img_num==json.slides.length)$(parent+' .slide_right img').hide();if(img_num==1)$(parent+' .slide_left img').hide();$(parent+' .current_slide_img').css('opacity',0).show().animate({opacity:1},200);}
function rail_size(parent){var rail_width=($(parent).width()-105);rail_width=Math.floor(rail_width/50)-1;return rail_width;}
function slideshow_pos(parent){var rhs=($(parent).parent().attr('id')=='right-col')?1:0;return rhs;}
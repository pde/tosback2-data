var GMapRegCb=(function(){var icons={paris:{size:14,src:'http://www.theregister.co.uk/Design/graphics/icons/vulture_bullet.png'},plane:{size:14,src:'http://regmedia.co.uk/2010/05/28/v1.png'},def:{size:32,src:'http://regmedia.co.uk/2008/10/17/spycar.png'}};var gicon;function set_gicon(type){var ico=icons[type];if(typeof ico=='undefined')ico=icons['def'];var iconsize=ico.size;gicon=new google.maps.MarkerImage(ico.src,new google.maps.Size(iconsize,iconsize),new google.maps.Point(0,0),new google.maps.Point(iconsize/2,iconsize/2));}
var info;var make_mark=function(map,d,type){var mark=new google.maps.Marker({position:d.latlng,icon:gicon,'map':map});google.maps.event.addListener(mark,"click",function(){var cls=typeof d.img=='undefined'?'':'google-reg-map-info';var div=$('<div style="min-height: 50px"></div>').addClass(cls)
.append($('<h5></h5>').text(d.name));if(typeof d.img!='undefined'){div.append($('<a></a>')
.attr({href:d.img,target:'_blank'})
.append($('<img></img>').attr({src:d.img,height:100,title:d.name}))
.append('<br>click for larger image'));}
if(typeof info=='undefined')info=new google.maps.InfoWindow();info.close();info.setContent($('<div></div>').append(div).html());info.open(map,mark);});};var link;var type;$(function(){var MapTypes=['spycar','paris-1','paris-2','paris-3','paris-4'];for(var map_type in MapTypes){link='#body a[rel=x-google'+MapTypes[map_type]+']';if($(link).length){type=MapTypes[map_type];$.getScript('http://dda.regmedia.co.uk/GMaps/'+MapTypes[map_type]+'.js',GMapRegCb);$('head').append('<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&amp;v=3&amp;&amp;callback=GMapRegCb"></script>');type=type.replace(/(paris)-[0123456789]+/,"$1");return;}}});var called=0;return function(){if(typeof RegArticle=='undefined')return;if(RegArticle['pf'])return;if(!called){called=1;return}

function RegSponsorship_settings(d){function set_default(obj,attr,val){if(!obj[attr])obj[attr]=val}
var url=location.host;var site_id=url.search('theregister.co.uk')!=-1?0:url.search('channelregister.co.uk')!=-1?1:2;var per_site_defaults=[{logo_alt:'The Register',logo_width:414,logo_height:80,mast_head_gradient:'/Design/graphics/std/masthead_gradient_80.png'},{logo_alt:'Channel Register',logo_width:395,logo_height:49,mast_head_gradient:'/Design/graphics/std/masthead_gradient.png'},{logo_alt:'reghardware',logo_width:300,logo_height:44,mast_head_gradient:''}];var site_defaults=per_site_defaults[site_id];set_default(d,'logo_slogan_width',site_defaults.logo_width);set_default(d,'logo_slogan_height',site_defaults.logo_height);set_default(d,'logo_slogan_alt',site_defaults.logo_alt);if(d.mast_head_gradient=='default')d.mast_head_gradient=site_defaults.mast_head_gradient;if(site_id==2){if(d.rh_page_bottom_vulture=='black')d.rh_page_bottom_vulture='/Design/graphics/icons/regIconBlack';if(d.rh_page_bottom_vulture=='white')d.rh_page_bottom_vulture='/Design/graphics/icons/regIcon';}
var defaults={mast_head_gradient:'',foot_gradient:'',sponsor_image_width:112,sponsor_image_height:60,sponsor_image_from_top:10,sponsor_image_from_right:121};for(i in defaults){set_default(d,i,defaults[i])}
return(d)}

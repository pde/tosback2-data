                                        $followButton.replaceWith(respObj['follow-button-html']);
                                    }

                                }

                            }
                        }

                        $this.data('clicked', false);
                    },

                    'error' : function(){
                        $this.data('clicked', false);
                    }
                   
                });

            }
         });

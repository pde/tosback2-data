                         if(count.length!=0)
                         var countVal = parseInt(count.text().match(/\d/)[0]);
                         if(typeof(countVal)=='number'){
                         count.text(de.bild.settings.community.lang.blockingNo.replace('%COUNT%',countVal-1));

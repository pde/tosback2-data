                $target = (target) ? $('#'+target) : data.$target;
                (typeof data.error === 'function') && data.error.apply(data.obj,[xhr, textStatus, $target]);
            }
        }); 
       
        return jhxr;
      };    

   /* public Interface*/
    return {
       'load' : function(data){
            if (data) {
                return parse(data);
            }
           return false; 
        }

   
    };
   
}(jQuery));

            $this.data('visible', visible); 
            
            var $placeHolder = $this.find('.sb-placeholder');

            if(!visible) {

                $this.find('iframe').remove();
                $this.find('.sb-to-fill').empty().hide();
                $placeHolder.show();
                $this.find('.social-lever').removeClass('active');
                return true;

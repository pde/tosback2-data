                var cb;
                if (!refresh) {
                    cb = function(){
                        renderUserBar(true);
                    }; 
                } else {
                    cb = function(){
                        window.location.reload();
                    };
                }  
                setUserInfo(jsonString, function(){
                    cb();
                });
        }
    }; 

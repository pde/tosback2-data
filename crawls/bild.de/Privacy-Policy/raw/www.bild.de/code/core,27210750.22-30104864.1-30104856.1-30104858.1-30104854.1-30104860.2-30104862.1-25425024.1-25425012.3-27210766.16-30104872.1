            FIST : de.bild.httpFist,
            UTIL_MODULE : de.bild.utils,
            MOBILE : de.bild.mobile,
            $COOKIE : $.cookie,
            $LIGHTBOX : $.fn.bildLightbox,
            LIB_MODULE : de.lib,
            SSO_MODULE : de.bild.sso,

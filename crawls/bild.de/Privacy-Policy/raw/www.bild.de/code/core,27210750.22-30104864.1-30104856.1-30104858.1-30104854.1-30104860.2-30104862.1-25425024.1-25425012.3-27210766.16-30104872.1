     * @param {Object} data
     * @private
     */
    function gatewayAutoLoginCallback(data) {
        $.ajax(de.bild.settings.community.urls.ssoIsLoggedIn, {
            dataType: 'json',
            cache : true
        }).done(function(data) {
             // remove iframe
            $('#'+ssoIframeId).remove();
            if(data.isLoggedIn === true){
                window.location.reload();

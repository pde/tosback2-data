    function updateVars(data) {
        if(!variables) {
            setVars();
        }
        if(data) {
            // intialise runtime functions
            for(var key in data) {
                // the variable must already exist and be of the same type
                if(variables.hasOwnProperty(key) && typeof variables[key] === typeof data[key]) {
                    variables[key] = data[key];
                }
            }
        }
        return variables;

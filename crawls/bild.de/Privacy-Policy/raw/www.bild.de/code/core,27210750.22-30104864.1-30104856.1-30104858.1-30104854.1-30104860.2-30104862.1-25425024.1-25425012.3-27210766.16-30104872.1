    function triggerGPlusButtons(visible, $context) {        
        
        var deInit = false;
        
        var $buttonContainer = $context.find('.social-button.gplus');
        
        var insertingButton = false;                
        
        $buttonContainer.each(function() {

            var $this = $(this);
            

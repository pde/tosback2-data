    function initAndBindSocialMediaBlocks() {
        
        $(document).on('mouseenter', '.social-button', function(){
            
            var $socialBlock = $(this);
            var socialBlock = this;
            
            if(!socialBlock.lever) {
                socialBlock.lever = $socialBlock.find('.social-lever');

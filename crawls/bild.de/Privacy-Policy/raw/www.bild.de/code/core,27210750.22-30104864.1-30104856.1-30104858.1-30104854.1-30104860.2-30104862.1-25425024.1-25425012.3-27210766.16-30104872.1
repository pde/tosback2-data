                        if($ugc.length) {
                            $ugc.show();
                        }
                    }).mouseleave(function() {
                        var $this = $(this), $ugc;
                        if($this.hasClass('mtA')) {
                            $ugc = $this.closest('div.ugcGroup');
                        } else {
                            $ugc = $this.find('div.ugcGroup');
                        }
                        if($ugc.length) {

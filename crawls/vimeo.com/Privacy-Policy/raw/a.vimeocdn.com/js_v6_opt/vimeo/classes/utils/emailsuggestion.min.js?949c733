var EmailSuggestion={emails:"hotmail.com,gmail.com,ymail.com,yahoo.com,yahoo.co.uk,yahoo.fr,aol.com,aim.com,msn.com,comcast.net,hotmail.co.uk,verizon.net,vimeo.com,bellsouth.net,me.com".split(","),check:function(a){answer=lev="";var b=100;if(!a||!a.indexOf("@"))return!1;a=a.split("@");EmailSuggestion.emails.each(function(c){if((lev=c.levenshtein(a[1]))<b)b=lev,answer=c});return 0<b&&4>b?[a[0],answer].join("@"):!1}};

	form.appendChild(input);
    input = createDOM("input", {type:"hidden",name:"firstEffort",value:"-1"});
	form.appendChild(input);
    input = createDOM("input", {type:"hidden",name:"item",value:"-1"});
	form.appendChild(input);
	input = createDOM("input", {type:"hidden",name:"eobFlag",value:"-1"});
	form.appendChild(input);

function loadViewer(elementId,docID, memID, width, height, printOnLoad, viewerLocation, key, pass,showBuyButton) {
		$j("#"+elementId).empty(); 
		if (key != undefined && key != null && key.length > 0) {
			url = "http://viewer.docstoc.com/?key=" + key + "&pass=" + pass;
		} else {
			url = "http://swf.docstoc.com/swf/" + viewerLocation;
		}
		
		var nobuybutton = 1-showBuyButton; //false meaning
		
		var embed = '<object id="_ds_' + docID + '" name="_ds_' + docID + '" width="' + width + '" height="' + height + 
		'" type="application/x-shockwave-flash" data="' + url + '"><param name="FlashVars" value="doc_id=' + docID + 
		'&mem_id=' + memID + '&showrelated=0&showotherdocs=0&doc_type=pdf&allowdownload=0&nobuybutton='+nobuybutton+'&printdoc=' + printOnLoad +
		'" /><param name="movie" value="' + url + '"/><param name="allowScriptAccess" value="always" /><param name="allowFullScreen" value="true" /></object>';
			$j("#"+elementId).append(embed);
	};

	function addGhostTextToInputMeta($obj, color1, color2) {
		$obj.focus(function() {
			if ($j(this).val() == $j(this).attr('title')) {
				$j(this).val('').css("color",color1);
			}
		});
		$obj.blur(function() {
			if ($j(this).val() == '') {
				$j(this).val($j(this).attr('title')).css("color",color2);
			}
		});
		$obj.blur().css("color",color2);		
	}

	function addGhostTextToInput($obj) {
		addGhostTextToInputMeta($obj, "black", "#BBB");
	}


function SubmitFeedBack(docID, vote) {
	$j.get("/handlers/UserFeedback.ashx", { doc_id: docID, helpfull: vote });
}
function SubmitPageFeedBack(vote) {
	$j.get("/handlers/UserFeedback.ashx", { doc_id: docID, helpfull: vote, url: escape(window.location.href) });
}

$j.fn.outerHTML = function() {
	return $j(this).clone().wrap('<div></div>').parent().html();
}

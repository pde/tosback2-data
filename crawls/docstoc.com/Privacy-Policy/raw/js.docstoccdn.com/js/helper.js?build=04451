function showImageToolTip(sender, imgSrc, id, top, left) {
	var defaultTop = 135;
	var defaultLeft = 20;

	if (top){ defaultTop = top;}
	if (left){ defaultLeft = left;}

	if ($j("#" + id).length != 0){
		var tip = $j("#" + id);
	}else{
		var tip = document.createElement("img");
		$j(tip).attr({
			'src' : imgSrc,
			'id' :  id
		}).css({
			'position' : 'absolute',
			'zIndex' : '1000'
		});
		$j('body').append(tip);
	}
	var sPos = findPos(sender);
	$j(tip).css({
		'display' : 'block',
		'top' : (sPos[1]-defaultTop) + 'px',
		'left' : (sPos[0]+defaultLeft) + 'px'
	});

	if($j.browser.msie && $j.browser.version==6.0) {
		setupIframe(tip,id);
	}
}

function toggleDropDowns(sender,type){
	var items = document.getElementsByTagName('select');
	for (var i=0;i<items.length;i++)
	{
		items[i].style.visibility = type;
	}
}

var myIFrame = null;

function setupIframe(tip,divId){
	var tip = $j("#" + tip);

	if (myIFrame == null){
		myIFrame = document.createElement('iframe');
		$j(myIFrame).attr({
			'id': divId + '_iframe',
			'src': "javascript:false;",
			'frameborder': '0'
		});

		//inject our iframe into the body of the document
		$j("body").append(myIFrame);
	}

	/* stylize it */
		$j(myIFrame).css({
	visibility : 'visible',
	  position : 'absolute',
	    filter : 'progid:DXImageTransform.Microsoft.Alpha(opacity = 0)',
	    zIndex : tip.css('zindex') - 1,
	     width : tip.width(),
	    height : tip.height(),
	       top : tip.css('top'),
	      left : Math.round( tip.css('left').replace('px','') ) + 10
		});
}

function hideImageToolTip(sender, id) {
	if($j.browser.msie && $j.browser.version==6.0 && myIFrame!=null) {
		myIFrame.style.visibility = 'hidden';
	}
	if ($j("#" + id).length !=0){
		$j("#" + id).hide();
	}
}

function endsWith(str, pattern) {
 var d = str.length - pattern.length;
 return d >= 0 && str.lastIndexOf(pattern) == d;
}

String.prototype.startsWith = function(str) {return (this.match("^"+str)==str);};


function DisableScreen2(id,color,opacity){

	var id      = id      || 'blackbg';
	var color   = color   || '000000';
	var opacity = (opacity && typeof(opacity) === "number" && opacity <= 1) ? opacity : .7 || .7;
	var ieOpacity = 100 * opacity;

	var dom = (id.substring(0) === "#" || id.substring(0) === ".") ? id : "#" + id;
	color = (color.substring(0) === "#") ? color : "#" + color;

	if ($j(dom).length != 0) {
		$j(dom).css({ 'display': 'block' });
		return; 
	}

	$j("body").append("<div id='" + id + "'></div>");

	$j(id).css({
		width          : '100%',
		height         : '100%',
		position       : 'fixed',
		top            : '0',
		left           : '0',
		backgroundColor: color,
		zIndex         : 888888,
		opacity        : opacity,
		'-ms-filter': 'progid:DXImageTransform.Microsoft.Alpha(Opacity=' + ieOpacity + ')',
		'filter': 'alpha(opacity=' + ieOpacity + ')'
	});


}

function DisableScreen(id,color,opacity){
    var cssId = id;
    if (cssId != null && cssId != "") {
        if (cssId.charAt(0) != '#') {
            cssId = '#' + cssId;
        }
    }
    if ($j(cssId).length != 0) {
        $j(cssId).css({ 'display': 'block' });
     return; }

     var ieOpacity =parseFloat(opacity);
     if (ieOpacity == "NaN") {

         ieOpacity = 70;
     }
     else {
         ieOpacity = ieOpacity * 100;
     }
	//create translucent background
	var bg = document.createElement('div');
	bg.id = id;
	$j(bg).css({
		width          : '100%',
		height         : '100%',
		position       : 'fixed',
		top            : '0',
		left           : '0',
		backgroundColor: color,
		zIndex         : 999999,
		opacity        : opacity,
		'-ms-filter': 'progid:DXImageTransform.Microsoft.Alpha(Opacity=' + ieOpacity + ')',
		'filter': 'alpha(opacity=' + ieOpacity + ')'
	});
	$j("body").append(bg);
}

function KeyboardFilter(e, sender, allowed, notallowed) {
	if (window.event) // IE
	{
		keyPressed = event.keyCode;
	}
	else if(e.which) // Netscape/Firefox/Opera
	{
		keyPressed = e.which;
	}
	if (keyPressed== 13 || keyPressed== 37 || keyPressed== 39 || keyPressed== 8 || keyPressed== 9 || keyPressed== 46){
		return true;
	}
	var keyValue = String.fromCharCode(keyPressed);
	if ((allowed!=null && allowed.indexOf(keyValue)>-1) || (allowed==null && notallowed.indexOf(keyValue)==-1)){
		return true;
	}else{
		sender.style.borderColor = 'red';
		setTimeout(function(){sender.style.borderColor = '#333333';},250);
	}
	return false;
}

function ShowSwf(url,width,height,container,flashvars,attributes) {
	var params = {allowScriptAccess: "always",allowFullScreen:"true",wmode:"transparent"};
	if (attributes==undefined) {
		attributes = {name:container};
	}
	swfobject.embedSWF(url, container, width, height, "9.0.0", null, flashvars, params, attributes);

	var ver = swfobject.getFlashPlayerVersion().major;
	if (ver ==0 ||ver<9) {
		$j(container).html ="<div class=\"no-flash\" style=\"width:" + width + "px; margin:0px;padding:0px;\">For best experience you need to have Flash version 9 installed.<br />Please click <a href=\"http://www.adobe.com/shockwave/download/download.cgi?P1_Prod_Version=ShockwaveFlash\"  target=\"_blank\">here</a> to have the most recent Flash player installed on your computer.</div>";
	}
}

function preloadMyDocs(){
	var b=$j('body');
	if (b.length!=0) {
		var frame = document.createElement('iframe');
		frame.setAttribute('id', 'preload_iframe');
		frame.setAttribute('src', "/filemanager/preload.htm");
		frame.setAttribute('frameborder', '0');
		frame.setAttribute('style', 'width:1px; height:1px; overflow:hidden;');
		//inject our iframe into the body of the document
		b.append(frame);
		frame.style.visibility = 'visible';
		frame.style.position = 'absolute';
		frame.style.filter = 'progid:DXImageTransform.Microsoft.Alpha(opacity=0)';
		frame.style.width = "1";
		frame.style.height = "1";
	}
}


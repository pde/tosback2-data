else if(this.currentPage<0){this.currentPage=this.maxPage;this.list.css(this.settings.position,(-this.pageSize*(1+this.currentPage))+'px');}
return;};PagerScroller.prototype.paginate=function(e){this.autoRotateEnabled=false;var href=$(e.target);var clickParent=href.parent('a');if(clickParent.length){href=clickParent;}
if(href.attr('data-page')){this.goToPage(parseInt(href.attr('data-page')));}
return false;};PagerScroller.prototype.goToPage=function(pageIndex){if(this.animating){return;}

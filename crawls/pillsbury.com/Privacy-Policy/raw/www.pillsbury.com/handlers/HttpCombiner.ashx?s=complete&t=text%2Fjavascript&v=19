jQuery(document).ready(function () {
    var recipeDetailRegex = /\/recipes\/[^\/]+\/[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}/i;
    if (!document.location.href.match(recipeDetailRegex)) {
        ntptEventTag('ev=contentview&ActionType=View');
    }
});

